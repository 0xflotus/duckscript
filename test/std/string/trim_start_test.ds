
function test_empty
    output = trim_start ""

    assert_eq ${output} ""
end_function

function test_trimmed
    output = trim_start "test test"

    assert_eq ${output} "test test"
end_function

function test_start_untrimmed
    output = trim_start "    test test"

    assert_eq ${output} "test test"
end_function

function test_end_untrimmed
    output = trim_start "test test    "

    assert_eq ${output} "test test    "
end_function

function test_both_untrimmed
    output = trim_start "    test test    "

    assert_eq ${output} "test test    "
end_function
