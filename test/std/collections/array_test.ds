
function test_array_with_data
    arr = array 1 2 3

    len = array_length ${arr}
    assert_eq ${len} 3

    isempty = array_is_empty ${arr}
    assert_false ${isempty}

    released = release ${arr}
    assert ${released}
end

function test_array_no_data
    arr = array

    len = array_length ${arr}
    assert_eq ${len} 0

    isempty = array_is_empty ${arr}
    assert ${isempty}

    released = release ${arr}
    assert ${released}
end
